<div>
  <%= @serie.publisher.name %>
  >
  <%= link_to @serie do %>
    <%= @serie.title %>
  <% end %>
</div>
<div class="serieShow_Container">
  <div class="serieShow_Metadata">
    <h1><%= @serie.title %></h1>
  </div>
  <% @serie.issues.each do |issue| %>
    <%= link_to(image_tag("#{issue.external_image_url}", class: "SeriesIndex_IssueCover responsive-img"), issue_path(issue)) %>
  <% end %>
</div>
